{
    "name": "date-time-picker-app",
    "version": "0.0.0",
    "description": "Angular Date Time Picker",
    "keywords": [
        "Angular",
        "datepicker",
        "date picker",
        "timepicker",
        "time picker"
    ],
    "author": "daniel pan <myron.yk.pan@gmail.com>",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build picker",
        "test": "ng test picker",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build_lib": "ng build picker && cp README.md dist/picker",
        "build_css": "mkdir -p dist/picker/assets/style && node-sass --output-style=compressed projects/picker/src/sass/picker.scss > dist/picker/assets/style/picker.min.css",
        "npm_pack": "cd dist/picker && npm pack",
        "package": "npm run build_lib && npm run build_css && npm run npm_pack",
        "postinstall": "ngcc"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/DanielYKPan/date-time-picker.git"
    },
    "dependencies": {
        "@angular/animations": "~9.1.13",
        "@angular/common": "~9.1.13",
        "@angular/compiler": "~9.1.13",
        "@angular/core": "~9.1.13",
        "@angular/forms": "~9.1.13",
        "@angular/platform-browser": "~9.1.13",
        "@angular/platform-browser-dynamic": "~9.1.13",
        "@angular/router": "~9.1.13",
        "core-js": "^2.6.11",
        "moment": "^2.27.0",
        "moment-timezone": "^0.5.31",
        "rxjs": "~6.6.0",
        "tslib": "^1.13.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.901.15",
        "@angular-devkit/build-ng-packagr": "~0.901.15",
        "@angular/cdk": "^8.2.3",
        "@angular/cli": "~9.1.15",
        "@angular/compiler-cli": "~9.1.13",
        "@angular/language-service": "~9.1.13",
        "@types/jasmine": "^3.3.16",
        "@types/jasminewd2": "^2.0.8",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.1.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.5",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "ng-packagr": "^9.0.0",
        "node-sass": "^4.14.1",
        "protractor": "^5.4.4",
        "ts-node": "^8.1.1",
        "tslint": "~5.16.0",
        "typescript": "~3.8.3"
    }
}
